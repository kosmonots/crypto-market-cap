{
    "name": "webberdoo/frework",
    "description": "FreWork is a PHP Micro Framework built using Symfony. Run - composer create-project webberdoo/frework YOUR-FOLDER dev-master",
    "homepage": "https://github.com/webberdoo/frework",
    "keywords": [
        "framework", "symfony"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "symfony/symfony": "^3",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.7",
        "twig/twig": "^2.4",
        "stof/doctrine-extensions-bundle": "^1.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "sensio/distribution-bundle": "^5.0",
        "vlucas/phpdotenv": "^2.4",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "jms/serializer-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^2.6"

    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    },
    "authors": [
        {
            "name": "Freddie Annobil",
            "email": "dev@webberdoo.co.uk"
        }
    ]
}
